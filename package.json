{
  "name": "wanderson-alves",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://wandersonalves.github.io/",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "files": [
